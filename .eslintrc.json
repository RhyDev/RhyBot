{
    "extends": "aqua",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "impliedStrict": true
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "max-len": "off",
        "no-undef": "off",
        "prefer-destructuring": "off",
        "max-depth": "off",
        "complexity": "off",
        "no-warning-comments": "off",
        "handle-callback-err": "off",
        "no-shadow": "off",
        "no-return-assign": "off",
        "no-process-env": "off"
    }
}